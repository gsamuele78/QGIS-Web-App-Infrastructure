# System User Configuration
PUID=1000
PGID=1000

# Domain and Network
DOMAIN=qgis.yourdomain.duckdns.org
DUCKDNS_SUBDOMAIN=qgis
DUCKDNS_TOKEN=your_duckdns_token

# Database Configuration
QWC_DB_NAME=qgis_sso_db
QWC_DB_USER=qgis_admin
QWC_DB_PASSWORD=generate_strong_password_here
QWC_DB_HOST=qwc-database
QWC_DB_PORT=5432

# Authentication Configuration
AUTH_SECRET_KEY=generate_256bit_secret_key_here
SESSION_TIMEOUT_MINUTES=120
PASSWORD_SALT=generate_unique_salt_here

# Security Settings
SSL_CERTIFICATE_EMAIL=your_email@example.com
ALLOWED_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION_MINUTES=30

# Application-Specific Configurations
QGIS_DESKTOP_MEMORY_LIMIT=2G
FILE_BROWSER_MAX_UPLOAD_SIZE=1G
